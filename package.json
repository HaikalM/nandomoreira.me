{
  "name": "nandomoreira.me",
  "version": "4.0.0",
  "description": "My superfast website and blog using Nuxt.js",
  "author": "Fernando Moreira <nandomoreira.me@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "clean": "rimraf .nuxt && rimraf dist",
    "build": "nuxt build",
    "start": "npm run build && nuxt start",
    "generate": "npm run clean && NODE_ENV=production nuxt generate",
    "deploy": "npm run generate && push-dir --dir=dist --branch=master --cleanup --force",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "echo \"no has tests lol\""
  },
  "dependencies": {
    "@nuxtjs/axios": "latest",
    "@nuxtjs/google-analytics": "^1.0.0",
    "nuxt": "latest",
    "nuxtent": "latest"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsdom": "^11.2.0",
    "node-sass": "^4.5.3",
    "push-dir": "^0.4.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6"
  },
  "eslintIgnore": [
    "src/plugins"
  ]
}
